<body>

    <h1 class="adoptAnimals__title">Animals Available for Adoption

    </h1>

    <section class="adoptAnimals__list">
        {{#each animals}}

        <article class="adoptAnimals__item">
            <h3 class="adoptAnimals__name">
                {{this.name}}

            </h3>

            <img src="/assets/{{this.name}}.svg" alt={{this.name}} class="adoptAnimals__image">
        </article>
        {{/each}}
    </section>

</body>